{
  "name": "sin",
  "publisher": "iggyzuk",
  "displayName": "sin",
  "description": "Search for symbol under cursor in workspace",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:iggyzuk/vscode-sin.git"
  },
  "contributes": {
    "commands": [
    {
      "command": "sin.searchSymbol",
      "title": "Search for Symbol"
    },
    {
      "command": "sin.closeQuickOpenAndFocusEditor",
      "title": "Close Quick Open and Focus Editor"
    }],
    "keybindings": [
    {
      "key": "escape",
      "command": "sin.closeQuickOpenAndFocusEditor",
      "when": "!inputFocus"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.32.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
